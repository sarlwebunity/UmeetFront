{
    "name": "Starterkit-JavaScript",
    "version": "0.1.2",
    "description": "Starterkit JavaScript",
    "license": "MIT",
    "homepage": "https://github.com/OIA-Sopra-Steria/starterkit-javascript",
    "ignore": [
        "**/.*",
        "node_modules",
        "bower_components",
        "test",
        "tests"
    ],
    "devDependencies": {
        "angular-mocks": "~1.5.x",
        "sinon": "http://sinonjs.org/releases/sinon-1.12.1.js",
        "bardjs": "~0.0.8"
    },
    "dependencies": {
        "jquery": "~2.1.1",
        "angular": "~1.5.x",
        "angular-sanitize": "~1.5.x",
        "bootstrap": "~3.3.1",
        "font-awesome": "~4.2.0",
        "moment": "~2.8.4",
        "angular-ui-router": "~0.2.13",
        "toastr": "~2.1.0",
        "angular-animate": "~1.5.x",
        "angular-bootstrap": "~1.2.2",
        "angular-localforage": "~1.2.3",
        "angular-busy": "~4.1.3",
        "angular-resource": "~1.5.x",
        "angular-touch": "~1.5.x",
        "angular-i18n": "^1.5.8",
        "ng-table": "~0.8.3"
    },
    "overrides": {
        "bootstrap": {
            "main": [
